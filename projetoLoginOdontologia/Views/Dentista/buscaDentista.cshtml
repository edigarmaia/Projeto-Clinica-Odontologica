@model projetoLoginOdontologia.Models.modelDentista
@{
    ViewBag.Title = "buscaDentista";
}

<h2>Busca Dentista</h2>

@using (Html.BeginForm("buscaDentista", "Dentista", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <label>Buscar Dentista</label>
    <br />
    <div class="editor-label">
        Digite o código da dentista a ser buscado:
    </div>
    <div class="editor-field">
        @Html.EditorFor(m => m.CodDentista)
    </div>
    <br />
    <input type="submit" value="Buscar" />
}

@Html.Raw(ViewBag.GridViewString)
